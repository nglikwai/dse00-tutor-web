{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Attach Program",
      "skipFiles": ["<node_internals>/**"],
      "port": 9229
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "Debug Chrome on localhost",
      "url": "http://hkdev.com:3002",
      "webRoot": "${workspaceFolder}",
      "sourceMapPathOverrides": {
        "webpack://_N_E/*": "${webRoot}/*"
      }
    }
  ]
}
